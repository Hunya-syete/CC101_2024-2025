strings = ["Animals", "Badger", "Honey Bee", "Honey Badger"] 
for string in strings: 
lowercase_string = string.lower() 
print(lowercase_string) 

animals
badger
honey bee
honey badger

strings = ["Animals", "Badger", "Honey Bee", "Honey Badger"] 
for string in strings: 
uppercase_string = string.upper() 
print(uppercase_string) 

ANIMALS 
BADGER
HONEY BEE
HONEY BADGER


